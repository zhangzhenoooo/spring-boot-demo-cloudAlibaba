<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhangz.demo.spring.cloud.platform.dao.BannerMapper">

    <sql id="tableName">banner</sql>

    <select id="getMaxId" resultType="java.lang.Integer">
        select COALESCE(MAX(id), 10000) from
        <include refid="tableName"/>
    </select>

    <select id="pageList" resultType="com.zhangz.demo.spring.cloud.platform.entity.Banner">
        select * from
        <include refid="tableName"/>
    </select>

</mapper>
