<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhangz.demo.spring.cloud.product.dao.GoodPropertyMapper">

    <sql id="tableName">good_property</sql>
    <select id="queryByPropertyId" resultType="com.zhangz.demo.spring.cloud.product.entity.GoodProperty">
        select * from
        <include refid="tableName"/>
        where property_id = #{propertyId,jdbcType=VARCHAR}
    </select>

</mapper>
