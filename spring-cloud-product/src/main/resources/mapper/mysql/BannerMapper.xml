<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhangz.demo.spring.cloud.product.dao.BannerMapper">

    <sql id="tableName">banner</sql>
    <select id="listActive" resultType="com.zhangz.demo.spring.cloud.product.entity.Banner">
        SELECT *
        FROM
        <include refid="tableName"/>
        where status = 0
        LIMIT 10;
    </select>


</mapper>
